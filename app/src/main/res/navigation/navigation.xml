<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/profileFragment2">

    <fragment
        android:id="@+id/profileFragment2"
        android:name="id.my.satria.portofolio_modul.ui.ProfileFragment2"
        android:label="profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/androidFragment2"
        android:name="id.my.satria.portofolio_modul.ui.AndroidFragment"
        android:label="fragment_android"
        tools:layout="@layout/fragment_android" />
    <fragment
        android:id="@+id/settingFragment2"
        android:name="id.my.satria.portofolio_modul.ui.SettingFragment"
        android:label="fragment_setting"
        tools:layout="@layout/fragment_setting" />
    <fragment
        android:id="@+id/uiUxFragment3"
        android:name="id.my.satria.portofolio_modul.ui.UiUxFragment"
        android:label="fragment_ui_ux"
        tools:layout="@layout/fragment_ui_ux" />
    <fragment
        android:id="@+id/flutterFragment2"
        android:name="com.example.portofolio.ui.FlutterFragment"
        android:label="fragment_flutter2"
        tools:layout="@layout/fragment_flutter2" />
    <fragment
        android:id="@+id/experienceFragment3"
        android:name="com.example.portofolio.ui.ExperienceFragment"
        android:label="fragment_experience"
        tools:layout="@layout/fragment_experience">
        <action
            android:id="@+id/action_experienceFragment3_to_experienceDetailFragment2"
            app:destination="@id/experienceDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/experienceDetailFragment"
        android:name="com.example.portofolio.ExperienceDetailFragment"
        android:label="Detail"
        tools:layout="@layout/fragment_experience_detail" >
        <argument
            android:name="experience"
            app:argType="com.example.portofolio.Portofolio" />
    </fragment>
</navigation>